{
  "name": "layowt-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/dashboard",
    "apps/marketing"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:dashboard": "turbo dev --filter=layowt-dashboard",
    "dev:marketing": "turbo dev --filter=layowt-marketing",
    "build": "npx prisma generate && next build",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "prettier-check": "prettier --check \"**/*.{ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "stripe:listen": "stripe listen --forward-to localhost:4343/api/webhooks/stripe",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@radix-ui/themes": "^2.0.3",
    "@reduxjs/toolkit": "^2.2.1",
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/auth-helpers-nextjs": "^0.9.0",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tabler/icons-react": "^3.1.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "framer-motion": "^11.0.20",
    "lucide-react": "^0.323.0",
    "micro-cors": "^0.1.1",
    "mini-svg-data-uri": "^1.4.4",
    "next": "^14.1.3",
    "next-redux-wrapper": "^8.1.0",
    "next-themes": "^0.2.1",
    "react": "^18",
    "react-dom": "^18",
    "react-fast-marquee": "^1.6.3",
    "react-redux": "^9.1.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "server-only": "^0.0.1",
    "simplex-noise": "^4.0.1",
    "sonner": "^1.4.0",
    "stripe": "^14.15.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "typewriter-effect": "^2.21.0",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-redux": "^7.1.33",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prisma": "^5.10.2",
    "stripe-cli": "^0.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
